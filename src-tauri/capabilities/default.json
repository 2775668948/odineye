{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*.tauri.app"
        },
        {
          "url": "http://www.baidu.com/*"
        }
      ],
      "deny": [
        {
          "url": "https://private.tauri.app"
        }
      ]
    }
  ]
}