{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "odineye",
  "version": "0.1.0",
  "identifier": "odineye.uk",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "奥丁之眼",
        "width": 1320,
        "height": 720,
        "resizable": false,
        "fullscreen": false,
        "decorations": true,
        "alwaysOnTop": false,
        "visible": true
      },
      {
        "label": "record",
        "title": "Second",
        "width": 600,
        "height": 350,
        "x": 10,
        "y": 20,
        "resizable": true,
        "fullscreen": false,
        "decorations": false,
        "alwaysOnTop": false,
        "transparent": true,
        "visible": false,
        "url": "/record"
      },
      {
        "label": "gamestart",
        "title": "gamestart",
        "width": 800,
        "height": 400,
        "x": 0,
        "y": 0,
        "resizable": true,
        "fullscreen": false,
        "decorations": false,
        "alwaysOnTop": false,
        "transparent": true,
        "visible": false,
        "url": "/gamestart"
      },
      {
        "hiddenTitle": true,
        "title": "奥丁之眼--oding.uk",
        "label": "overlay",
        "url": "/gameing",
        "transparent": true,
        "decorations": false,
        "fullscreen": false,
        "alwaysOnTop": true,
        "resizable": false,
        "focus": false,
        "titleBarStyle": "Overlay",
        "width": 2000,
        "height": 1300,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "msi",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "wix": {
        "language": "zh-CN"
      }
    }
  },
  "plugins": {}
}